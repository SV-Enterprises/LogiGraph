package cdac.project.logigraph.routing.service;

import java.util.List;

public interface RoutingService {

    /**
     * Finds the nearest warehouse from a list of candidate warehouses
     * using graph-based routing (Dijkstra).
     *
     * @param sourceWarehouseId      warehouse where routing starts
     * @param candidateWarehouseIds  warehouses that can fulfill the order
     * @return nearest warehouse id
     */
    Integer findNearestWarehouse(
            Integer sourceWarehouseId,
            List<Integer> candidateWarehouseIds
    );
}


RoutingServiceImpl :

package cdac.project.logigraph.routing.service;

