    Vehicle assignVehicle(Integer warehouseId);
}


VehicleServiceImpl:

package cdac.project.logigraph.vehicle.service;

import org.springframework.stereotype.Service;

import cdac.project.logigraph.vehicle.entity.Vehicle;
import cdac.project.logigraph.vehicle.enums.VehicleStatus;
import cdac.project.logigraph.vehicle.repository.VehicleRepository;
import jakarta.transaction.Transactional;

@Service
public class VehicleServiceImpl implements VehicleService {

    private final VehicleRepository vehicleRepository;

    public VehicleServiceImpl(VehicleRepository vehicleRepository) {
        this.vehicleRepository = vehicleRepository;
    }

    @Override
    @Transactional
    public Vehicle assignVehicle(Integer warehouseId) {

        if (warehouseId == null) {
            throw new IllegalArgumentException("Warehouse id cannot be null");
        }

        Vehicle vehicle = vehicleRepository
                .findFirstByStatusAndCurrentWarehouseId(
                        VehicleStatus.IDLE,
                        warehouseId
                )
                .orElseThrow(() ->
                        new IllegalStateException(
                                "No available vehicles at warehouse " + warehouseId
                        )
                );

        vehicle.setStatus(VehicleStatus.ASSIGNED);

        return vehicle;
    }
}

